// ----------------- RECUPERATION :

composer74 install / composer74 update
npm run dev
yarn run encore dev

// ----------------- INSTALLATION :

cmd : composer74 create-project symfony/website-skeleton=^4.4 read_and_write
cmd : cd ./read_and_write
cmd : php bin/console cache:clear --env=dev
action : composer.json > allow-contrib = true
-- package de base
cmd : composer74 require symfony/apache-pack
cmd : composer74 require symfony/maker-bundle
cmd : composer74 require symfony/profiler-pack
cmd : composer74 require symfony/twig-bundle
-- pour les assets
cmd : composer74 require symfony/webpack-encore-bundle
cmd : yarn add jquery
cmd : yarn add sass-loader@^9.0.1 sass --dev
cmd : yarn install
cmd : npm run dev || yarn encore dev
action = modif sur webpack.config.js && modif sur assets/app.js && modif sur assets/styles et assets/scripts
-- pour la base de données
cmd : composer74 require symfony/orm-pack
action : configuration du fichier .env
cmd : php74 bin/console doctrine:database:create




